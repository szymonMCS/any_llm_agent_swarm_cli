[run]
source = agentswarm
branch = True
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */.venv/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    pass
    \.\.\.
    @abstractmethod

show_missing = True
fail_under = 100

[html]
directory = htmlcov

[xml]
output = coverage.xml
